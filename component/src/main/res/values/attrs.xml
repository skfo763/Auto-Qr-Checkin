<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PermissionStateView">
        <attr name="psv_title" format="reference|string"/>
        <attr name="psv_subtitle" format="reference|string"/>
        <attr name="psv_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="NumberIconTextView">
        <attr name="num_icon_tv_number" format="integer"/>
        <attr name="num_icon_tv_title" format="reference|string"/>
        <attr name="num_icon_tv_subtitle" format="reference|string"/>
    </declare-styleable>

    <declare-styleable name="YouTubePlayerView">
        <attr name="videoId" format="string|reference" />
        <attr name="fragment" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="CheckStateLayout">
        <attr name="nonCheckedBackground" format="color"/>
        <attr name="checkedBackground" format="color"/>
    </declare-styleable>
</resources>